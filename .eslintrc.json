{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"semi": "error",
		"import/no-extraneous-dependencies": [
			"off",
			{
				"devDependencies": true
			}
		],
		"import/extensions": ["error", "always"]
	}
}
